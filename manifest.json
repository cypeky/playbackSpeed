  {
    "name": "Youtube custom speed",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
	"content_scripts":[
		{
			"matches":["<all_urls>"],
			"js":["popup.js"]
		}
	],
	"browser_action":{
		"default_popup":"popup.html",
		"default_title":"Speed"
		}
  }
  